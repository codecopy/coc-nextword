{
  "name": "coc-nextword",
  "version": "0.1.0",
  "description": "nextword extension for coc.nvim",
  "author": "Heyward Fann <fannheyward@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.70"
  },
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npm-run-all clean build"
  },
  "devDependencies": {
    "@types/node": "^13.9.2",
    "@types/which": "^1.3.2",
    "coc.nvim": "^0.0.77",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "which": "^2.0.2"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-nextword configuration",
      "properties": {
        "nextword.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-nextword extension"
        },
        "nextword.dataPath": {
          "type": "string",
          "default": "",
          "description": "Data path of nextword dataset. Use $NEXTWORD_DATA_PATH by default"
        },
        "nextword.count": {
          "type": "number",
          "default": 10,
          "description": "Max candidates word number"
        }
      }
    },
    "commands": []
  }
}
